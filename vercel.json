{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "bun run --filter @cadhy/web build",
  "outputDirectory": "apps/web/dist",
  "installCommand": "bun install",
  "framework": "vite",
  "ignoreCommand": "npx turbo-ignore @cadhy/web",
  "rewrites": [
    {
      "source": "/((?!assets/|hero/|favicon|logo|sitemap|robots|manifest).*)",
      "destination": "/index.html"
    }
  ]
}
